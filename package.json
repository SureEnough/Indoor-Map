  "dependencies": {
    "dat.gui": "^0.7.1",
    "stats.js": "^0.17.0",
    "three": "^0.91.0"
  },
  "devDependencies": {
    "@types/three": "^0.91.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.1.0",
    "typedoc": "^0.11.1",
